@model SitiosWeb.Model.Configuraciones

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Borrar</title>
</head>
<body>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Configuraciones</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NomConfiguracion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomConfiguracion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ValorConfig)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ValorConfig)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NumConfig)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NumConfig)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdConfiguraciones" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        
    </form>
</div>
</body>
</html>
